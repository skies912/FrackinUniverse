			"metals_alloy2"	: [ "Advanced Alloy",	"^orange;Tier 4^reset;\n\nLearn to process Advanced Alloy." ],
			"metals_alloy3"	: [ "Dense Alloy",	"^orange;Tier 4^reset;\n\nLearn to process Dense Alloy." ],
